<script>
    StackExchange.ready(function() {

        var location = 0;
        if ($("body").hasClass("questions-page")) {
            location = 2;
        } else if ($("body").hasClass("question-page")) {
            location = 9;
        } else if ($("body").hasClass("home-page")) {
            location = 1;
        } else if ($("body").hasClass("unanswered-page")) {
            location = 7;
        } else if ($("body").hasClass("user-page")) {
            if ($("#user-card").length) {
                location = 4;
            }
            else if ($("#top-cards").length){
                location = 5;
            }
            else if ($(".timeline").length) {
                location = 6;
            }
        }

        $(".ds-announcement .js-dismiss").one("click", function() {
            $(".ds-announcement").fadeOut("fast");
            $.post("/users/story/dismiss-announcement-hero", { fkey: StackExchange.options.user.fkey, location: location });
        });

        StackExchange.using("gps", function () {
            StackExchange.gps.track("story.hero.show", { location: location }, true);
        });

    });
</script><div id="mainbar">

